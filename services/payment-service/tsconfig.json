{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "prisma/*": ["prisma/*"]
    }
  },
  "include": ["src/**/*", "prisma/**/*",  "src/types.d.ts"
],
  
}
